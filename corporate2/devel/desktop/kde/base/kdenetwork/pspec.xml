    <Package>
        <Name>kdenetwork-krfb</Name>
        <PartOf>network.remoteshell</PartOf>
        <Summary>VNC Server</Summary>
        <Description>KRfb is a small server for the RFB protocol, better known as VNC. Unlike most RFB servers, KRfb allows you to share your X11 session. It's used for KDE's desktop sharing feature.</Description>
        <RuntimeDependencies>
            <Dependency>kdelibs</Dependency>
            <Dependency>qt3</Dependency>
            <Dependency>openslp</Dependency>
            <Dependency>libXtst</Dependency>
            <Dependency>libXext</Dependency>
        </RuntimeDependencies>
        <Replaces>
            <Package>kdenetwork</Package>
        </Replaces>
        <Files>
            <Path fileType="executable">/usr/kde/3.5/bin/krfb*</Path>
            <Path fileType="library">/usr/kde/3.5/lib/kde3/kcm_krfb.*</Path>
            <Path fileType="library">/usr/kde/3.5/lib/kde3/kded_kinetd.*</Path>
            <Path fileType="data">/usr/kde/3.5/share/applications/kde/*krfb.desktop</Path>
            <Path fileType="data">/usr/kde/3.5/share/apps/kinetd/eventsrc</Path>
            <Path fileType="data">/usr/kde/3.5/share/apps/krfb</Path>
            <Path fileType="data">/usr/kde/3.5/share/icons/*/*/apps/krfb.png</Path>
            <Path fileType="data">/usr/kde/3.5/share/services/kded/kinetd.desktop</Path>
            <Path fileType="data">/usr/kde/3.5/share/services/kinetd_krfb*</Path>
            <Path fileType="data">/usr/kde/3.5/share/servicetypes/kinetdmodule.desktop</Path>
            <Path fileType="doc">/usr/kde/3.5/share/doc/HTML/en/krfb</Path>
        </Files>
    </Package>
