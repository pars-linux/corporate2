+			tkstring);
 		exit(1);
 	}
 
 	retval = krb5_cc_initialize(context, ccache, my_principal);
 	if (retval) {
 		com_err (progname, retval, "when initializing cache %s",
-			 buf);
+			 tkstring);
 		exit(1);
 	}
 
