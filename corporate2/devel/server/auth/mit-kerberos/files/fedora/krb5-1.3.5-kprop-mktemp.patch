-	sprintf(buf, "FILE:%s", tkstring);
-
-	retval = krb5_cc_resolve(context, buf, &ccache);
+	retval = krb5_cc_resolve(context, tkstring, &ccache);
 	if (retval) {
 		com_err(progname, retval, "while opening credential cache %s",
