+#: rc.cpp:1508
+#, no-c-format
+msgid "Allow use of short blocks"
+msgstr "Kısa blokların kullanımına izin ver"
+
+#. i18n: file ./plugins/lame/lameencoderdialogbase.ui line 1195
+#: rc.cpp:1511
+#, no-c-format
+msgid "Use only short blocks"
+msgstr "Sadece kısa bloklar kullan"
