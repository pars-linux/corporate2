            <Path fileType="data">/usr/kde/3.5/share/icons/hicolor/*/apps/kuickshow.png</Path>
            <Path fileType="doc">/usr/kde/3.5/share/doc/HTML/en/kuickshow</Path>
        </Files>
    </Package>
